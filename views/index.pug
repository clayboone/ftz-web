extends layout

block content
  .memory-box
    h3 Memory usage
    .info
      p.label Physical Usage
      ul.legend
        li
          .active
          p Active
        li
          .used
          p Used
        li
          .free
          p Free
    #memusage-container
      #memusage-used
      #memusage-active
    .info
      p.label Swap Space Usage
      ul.legend
        li.used Used
        li.free Free
    #swapusage-container
      #swapusage-used
