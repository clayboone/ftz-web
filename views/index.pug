extends layout

block content
  .memory-box
    h3 Memory usage
    p Physical:
      .legend-container
        ul.legend
          li.active Active
          li.used File Buffers & Cache
          li.free Free
    #memusage-container
      #memusage-used
      #memusage-active
    p Swap:
      .legend-container
        ul.legend
          li.used File Buffers & Cache
          li.free Free
    #swapusage-container
      #swapusage-used
